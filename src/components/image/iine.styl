@import './../../lib/variable/prefix.styl'
@import './../../lib/variable/functions.styl'

.{prefix}-image

    svg[aria-label='heart']
    svg[aria-label='heart-fill']
        color _rgb(var(--magenta-6))
        cursor pointer
    
    &[data-iine] svg[aria-label='heart']
    &:not([data-iine]) svg[aria-label='heart-fill']
        display none
